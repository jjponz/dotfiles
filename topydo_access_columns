[Doing]
title = Doing
filterexpr = +access @doing

[Today]
title = Today
groupexpr = due
filterexpr = +access due:today

[Inbox]
title = Inbox
filterexpr = +access
groupexpr = context

[Without date]
title = To Do sin fechas
filterexpr = +access -due: -@week -t: -url: 
groupexpr = context due 

[Started]
title = Deben de hacerse durante el mes
filterexpr = +access t: 
show_all = 1

[Week]
title = Week
filterexpr = +access @week

[Overdue]
title = Overdue
filterexpr = due:<today +access
groupexpr = due
sortexpr = due, desc:priority

[Future]
title = Future
groupexpr = due
filterexpr = +access due:>today

[Waiting]
title = Waiting
filterexpr = +access @waiting
